generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             String   @id @default(uuid())
  email          String   @unique
  userName       String   @db.VarChar(20)
  phone          String   @unique
  hashedPassword String
  role           Role     @default(USER)
  isBlocked      Boolean  @default(false)
  ownedBikes     Bike[]   @relation(name: "CreatedByUser")
  likedBikes     Bike[]   @relation(name: "UserToBikeLikes")
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt

  @@map("users")
}

model Bike {
  id             String        @id @default(uuid())
  model          String        @db.VarChar(20)
  manufacturer   Manufacturer
  color          BikeColor
  engineCapacity Int
  horsePower     Int
  price          Prices[]
  year           Int
  used           Boolean
  isForParts     Boolean
  images         String[]      @default([])
  description    String        @default("")
  ownerId        String
  owner          User          @relation(fields: [ownerId], references: [id], name: "CreatedByUser")
  location       String        @db.VarChar(100)
  likedByUsers   User[]        @relation(name: "UserToBikeLikes")
  createdAt      DateTime      @default(now())
  updatedAt      DateTime      @updatedAt
  listingStatus  ListingStatus @default(PENDING_APPROVAL)

  @@map("bikes")
}

model Prices {
  id        String   @id @default(uuid())
  bike      Bike     @relation(fields: [bikeId], references: [id], onDelete: Cascade)
  bikeId    String
  price     Decimal  @db.Decimal(10, 2)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum ListingStatus {
  PENDING_APPROVAL
  ACTIVE
  SOLD
  DRAFT
  UNACTIVE
}

enum BikeColor {
  BLACK
  WHITE
  RED
  BLUE
  GREEN
  YELLOW
  ORANGE
